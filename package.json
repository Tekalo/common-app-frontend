  "scripts": {
    "build-storybook": "storybook build -o .storybook/build",
    "build:for-edge": "pnpx @cloudflare/next-on-pages",
    "build": "next build",
    "cy": "cypress open",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "e2e:ci": "cypress run --e2e",
    "export": "next export",
    "lint": "next lint",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "pretest-cleanup": "rm -rf .next .nyc_output coverage ; mv _babelrc .babelrc",
    "prettier": "prettier --write .",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "start-test dev 3000 e2e:ci",
    "test:unit": "pnpm pretest-cleanup ; BABEL_ENV=unit cypress run --component ; mv .babelrc _babelrc",
    "wrangle": "pnpm build && pnpm start && pnpm wrangler pages dev ./out"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
